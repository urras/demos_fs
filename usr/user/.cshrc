set path=(. /bin /usr/bin /etc /usr/ucb )
set history=20
set prompt="user \!> "
alias a  alias
alias c  cat
alias e  echo
alias g  grep -n
alias h  history
alias l  ls -l
alias m  more
alias m0 mount /dev/fd0
alias m1 mount /dev/fd1
alias u0 umount /dev/fd0
alias u1 umount /dev/fd1
alias me1 mount /dev/fde1
alias me0 mount /dev/fde0
alias ue0 umount /dev/fde0
alias ue1 umount /dev/fde1
setenv TAPE /dev/fd1
setenv TERM ccgd
